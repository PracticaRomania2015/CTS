ALTER PROCEDURE [dbo].[AddAuditEvent]
	@UserId int,
	@Action varchar(1000),
	@DateTime datetime,
	@TicketId int

AS
BEGIN

	SET NOCOUNT ON;

	INSERT INTO Audit(UserId, Action, Datetime, TicketId)
	VALUES (@UserId, @Action, @DateTime, @TicketId)

END
